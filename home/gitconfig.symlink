[branch]
  autoSetupRebase = always
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  frag = magenta bold
  meta = yellow bold
  new = green bold
  old = red bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  editor = atom --wait
  excludesFile = $HOME/.gitignore
[credential]
  helper = osxkeychain
[user]
  name = Jason Garber
  email = jason@sixtwothree.org
